{
    "name": "simple-mern-stack-client",
    "version": "1.0.1",
    "description": "A Simple MERN Stack Boilerplate",
    "main": "index.js",
    "scripts": {
        "set_env_dev": "SET \"NODE_ENV=dev\"",
        "set_env_prod": "SET \"NODE_ENV=production\"",
        "start": "SET \"NODE_ENV=development\" && webpack-dev-server --config webpack.config.js --mode=development --open --hot \"true\" ",
        "build": "rimraf dist && SET \"NODE_ENV=production\" && webpack --mode=production --config webpack.config.js"
    },
    "dependencies": {
        "react": "^16.5.2",
        "react-dom": "^16.5.2",
        "react-router-dom": "^4.3.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.5",
        "@babel/core": "^7.1.2",
        "@babel/preset-env": "^7.1.0",
        "@babel/preset-react": "^7.0.0",
        "autoprefixer": "^9.2.1",
        "babel-loader": "^8.0.4",
        "babel-preset-react": "^6.24.1",
        "css-loader": "^1.0.0",
        "file-loader": "^2.0.0",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.4.4",
        "node-sass": "^4.9.4",
        "react-hot-loader": "^4.3.11",
        "sass-loader": "^7.1.0",
        "server": "^1.0.18",
        "style-loader": "^0.23.1",
        "webpack": "^4.21.0",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.9"
    }
}

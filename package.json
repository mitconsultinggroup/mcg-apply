{
  "name": "mcg-apply",
  "version": "1.0.0",
  "description": "Recruitment Platform for the MIT Consulting Group",
  "main": "server/index.js",
  "scripts": {
    "babel-node": "babel-node ./server/index.js",
    "dev": "concurrently \"npm run server\" \"cd client && npm start\"",
